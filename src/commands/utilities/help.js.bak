const { Command } = require('discord.js-commando');

module.exports = class HelpCommand extends Command {
	constructor(client) {
		super(client, {
            name: 'help',
			format: '<command>',
			group: 'utilities',
			memberName: 'command',
            description: 'Retrieves general help or detailed help for a given command.',
		});
	}

	run(message) {
        // message.reply('Help is in progress.');
        message.say(`${message.author}, I've sent help to you in a DM.`);
        
	}
};
